local Library = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()
local Window = Library.CreateLib("MiniHub V1", "LightTheme")

-- Tabs

local Tab1 = Window:NewTab("Bedwars")
local Tab1Section = Tab1:NewSection("Credits: Zephyl#5717")
local Tab1Section = Tab1:NewSection("Helpers: mody#7541")
-- Buttons/Windows/Idk

Tab1Section:NewButton("Inf Jumps", "Enables Inf Jumps", function()
    local InfiniteJumpEnabled = true
game:GetService("UserInputService").JumpRequest:connect(function()
	if InfiniteJumpEnabled then
		game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")
	end
end)
end)

Tab1Section:NewToggle("Fov", "Changes Fov", function(state)
    if state then
        game.Workspace.CurrentCamera.FieldOfView = 120
    else
        game.Workspace.CurrentCamera.FieldOfView = 80
    end
end)

Tab1Section:NewSlider("Speed", "Sussy Speed", 260, 120, function(v)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v
end)

Tab1Section:NewButton("Jn 2.0", "o", function()
    loadstring(game:HttpGet("https://cdn.discordapp.com/attachments/1002510858917773352/1002895558790361158/untitled.txt"))()
end)

Tab1Section:NewButton("Jn 10v", "o", function()
    loadstring(game:HttpGet(("https://raw.githubusercontent.com/YourLocalNzi/Ye/main/Jng10"),true))()
end)

Tab1Section:NewButton("Jn v9", "o", function()
    loadstring(game:HttpGet(('https://raw.githubusercontent.com/YourLocalNzi/Ye/main/Jn_V9.txt'),true))()
end)

Tab1Section:NewButton("suntari code is cypheriscool", "ButtonInfo", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Unknownuser11736/abcd/main/suntaivsystem", true))()
end)

